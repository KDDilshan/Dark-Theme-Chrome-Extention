{
    "name": "Extension Name",
    "version": "0.1",
    "manifest_version": 3,
    "description": "new chorme extention",
    "permissions": [],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./scripts/contentScript.js"],
            "css": ["./styles/styles.css"]
        }
    ],
    "background": {
        "service_worker": "./scripts/background.js"
    },
    "action": {
        "default_title": "chrome extention default_title",
        "default_popup": "popup.html"
    }
}